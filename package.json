{
  "name": "decomposer",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "bun ./src/index.ts",
    "dev": "bun --watch ./src/index.ts",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "test": "bun test ./src",
    "test:coverage": "bun test --coverage ./src",
    "check": "tsc --noEmit",
    "circular": "madge --circular ./src --extensions ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "description": "Lightweight TypeScript shim for the image-builder-cli",
  "dependencies": {
    "chalk": "4.1.2",
    "hono": "4.8.4",
    "http-status-codes": "2.3.0",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.30.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/bun": "1.2.18",
    "@types/node": "24.0.10",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "eslint": "9.30.1",
    "globals": "16.3.0",
    "madge": "8.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1"
  }
}
